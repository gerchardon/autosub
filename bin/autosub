#!/usr/bin/env node
"use strict";

process.bin = process.title = "autosub";

// Init logger
var winston = require('winston');
winston.cli();

var files = process.argv.slice(2);
winston.debug('Find subtitles for : %s', files, {});

// Execute for all plugins
var autosub = require('../lib');
// TODO Use manager for all plugins
// autosub().plugins.addic7ed.search('test.avi');
autosub().search('test.avi');
